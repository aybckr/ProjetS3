<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Compteserv</servlet-name>
        <servlet-class>org.servlet.Compteserv</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>org.servlet.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>chercherCommande</servlet-name>
        <servlet-class>org.servlet.chercherCommande</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>passerCommande</servlet-name>
        <servlet-class>org.servlet.passerCommande</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deletePlat</servlet-name>
        <servlet-class>org.servlet.deletePlat</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addPlat</servlet-name>
        <servlet-class>org.servlet.addPlat</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Compteserv</servlet-name>
        <url-pattern>/Compteserv</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>chercherCommande</servlet-name>
        <url-pattern>/chercherCommande</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>passerCommande</servlet-name>
        <url-pattern>/passerCommande</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deletePlat</servlet-name>
        <url-pattern>/deletePlat</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addPlat</servlet-name>
        <url-pattern>/addPlat</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
